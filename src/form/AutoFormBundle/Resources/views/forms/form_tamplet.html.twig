<form {% for key,value in form.attr %}{{ key }}="{{ value }}"{% endfor %}>
    {% if form.contents %}
        {% for content in form.contents %}
            {% if content.row %}<div{% for Key,value in content.row %} {{ key }} ="{{ value }}" {% endfor %}>{% endif %}
                {% for column in content.cols %}
                    {% if column.col %}<div{% for key,value in column.col %} {{ key }}="{{ value }}" {% endfor %}>{% endif %}
                    {% for field in column.fields  %}
                        {{ field | raw }}
                    {% endfor %}
                    {% if column.col %}</div>{% endif %}
                {% endfor %}
            {% if content.row %}</div>{% endif %}
        {% endfor %}
    {% endif %}
</form>